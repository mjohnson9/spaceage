@SolidClass = sa_environment :
	"A cuboid environment."
[
	name(string) : "Name" : "" : "The name of the environment"
	gravity(float) : "Gravity" : "1.0" : "Scale for effect of gravity. (1.0 = normal gravity)"
	temperature(float) : "Temperature" : "293.15" : "The temperature of this environment in Kelvin"

	habitable(Choices) : "Habitable" : 1 : "Control whether the environment is habitable" =
	[
		0 : "No"
		1 : "Yes"
	]
]
